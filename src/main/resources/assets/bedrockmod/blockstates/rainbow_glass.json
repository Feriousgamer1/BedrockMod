{
  "multipart": [
    {
      "apply": {
        "model": "bedrockmod:block/rainbow_glass_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "bedrockmod:block/rainbow_glass_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "bedrockmod:block/rainbow_glass_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "bedrockmod:block/rainbow_glass_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "bedrockmod:block/rainbow_glass_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "bedrockmod:block/rainbow_glass_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "bedrockmod:block/rainbow_glass_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "bedrockmod:block/rainbow_glass_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "bedrockmod:block/rainbow_glass_noside",
        "y": 270
      }
    }
  ]
}